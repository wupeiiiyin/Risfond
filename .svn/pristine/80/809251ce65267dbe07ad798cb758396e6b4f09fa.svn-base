package com.risfond.rnss.entry;

import java.math.BigDecimal;

/**
 * Created by Abbott on 2017/6/23.
 */

public class OperationPlatform {
    /**
     * ResumeCurrent : 0
     * HuikuanCurrent : 0
     * OfferCurrent : 0
     * QianyueCurrent : 0
     * RuzhiCurrent : 0
     */

    private int ResumeCurrent;
    private BigDecimal HuikuanCurrent;
    private int OfferCurrent;
    private int QianyueCurrent;
    private int RuzhiCurrent;

    public int getResumeCurrent() {
        return ResumeCurrent;
    }

    public void setResumeCurrent(int ResumeCurrent) {
        this.ResumeCurrent = ResumeCurrent;
    }

    public BigDecimal getHuikuanCurrent() {
        return HuikuanCurrent;
    }

    public void setHuikuanCurrent(BigDecimal HuikuanCurrent) {
        this.HuikuanCurrent = HuikuanCurrent;
    }

    public int getOfferCurrent() {
        return OfferCurrent;
    }

    public void setOfferCurrent(int OfferCurrent) {
        this.OfferCurrent = OfferCurrent;
    }

    public int getQianyueCurrent() {
        return QianyueCurrent;
    }

    public void setQianyueCurrent(int QianyueCurrent) {
        this.QianyueCurrent = QianyueCurrent;
    }

    public int getRuzhiCurrent() {
        return RuzhiCurrent;
    }

    public void setRuzhiCurrent(int RuzhiCurrent) {
        this.RuzhiCurrent = RuzhiCurrent;
    }
}
